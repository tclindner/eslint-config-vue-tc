{
  "name": "eslint-config-vue-tc",
  "version": "4.0.0",
  "description": "ESLint shareable config for Vue projects",
  "keywords": [
    "eslintconfig",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "eslint",
    "code style",
    "vue"
  ],
  "homepage": "https://github.com/tclindner/eslint-config-vue-tc",
  "bugs": {
    "url": "https://github.com/tclindner/eslint-config-vue-tc/issues"
  },
  "author": "Thomas Lindner",
  "repository": {
    "type": "git",
    "url": "https://github.com/tclindner/eslint-config-vue-tc.git"
  },
  "files": [
    "index.js",
    "rules",
    "CONTRIBUTING.md"
  ],
  "main": "index.js",
  "scripts": {
    "eslint": "eslint . --format=node_modules/eslint-formatter-pretty",
    "lint": "npm run npmpackagejsonlint && npm run eslint",
    "npmpackagejsonlint": "npmPkgJsonLint .",
    "test": "jest"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-tc": "^13.0.2",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vue-a11y": "^0.0.31",
    "is-plain-obj": "^2.1.0",
    "jest": "^26.1.0",
    "npm-package-json-lint": "^5.1.0",
    "npm-package-json-lint-config-tc": "^4.0.0",
    "prettier": "^2.0.5"
  },
  "peerDependencies": {
    "eslint": "^6.0.0",
    "eslint-plugin-vue": "^5.0.0 || ^6.0.0",
    "eslint-plugin-vue-a11y": "^0.0.31"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "license": "MIT"
}
